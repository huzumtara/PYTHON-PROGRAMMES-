# simple list 
my_list=[1,3,5,6,7]
print(my_list[4])
print(my_list[3])
print(my_list[1])
#modifying my list
print(my_list[1])==100
print(my_list)
#adding the value to the list 
my_list.append(80)
print(my_list)
#inserting the value in the list 
my_list.insert(2,40)
print(my_list)
#removing the value in the list 
my_list.remove(6)
print(my_list)
